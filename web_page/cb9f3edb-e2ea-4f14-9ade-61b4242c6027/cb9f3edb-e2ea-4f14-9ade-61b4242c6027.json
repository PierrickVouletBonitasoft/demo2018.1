    "createListOfPeopleForDropDown" : {
      "type" : "expression",
      "value" : "var people = [];\n\nfor(var i=1;i < $data.getPeople.length;i++) {\n    var person = {\"name\": $data.getPeople[i].firstname+\" \"+$data.getPeople[i].lastname,\n                    \"id\": $data.getPeople[i].id\n    };\n    people.push(person);\n}\n\nreturn people;",
      "exposed" : false
    },
