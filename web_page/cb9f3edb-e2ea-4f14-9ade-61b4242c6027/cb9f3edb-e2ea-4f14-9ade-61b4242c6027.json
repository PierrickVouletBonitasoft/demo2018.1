    },
    "getDensoProcessVersion" : {
      "type" : "expression",
      "value" : "if ($data.getDensoProcess && $data.getDensoProcess.length) {\r\n    var process = $data.getDensoProcess[0];\r\n    return process.version;\r\n}",
      "exposed" : false
    },
    "getDensoProcessName" : {
      "type" : "expression",
      "value" : "if ($data.getDensoProcess && $data.getDensoProcess.length) {\r\n    var process = $data.getDensoProcess[0];\r\n    return process.name;\r\n}",
      "exposed" : false
    },
    "getDensoProcessId" : {
      "type" : "expression",
      "value" : "if ($data.getDensoProcess && $data.getDensoProcess.length) {\r\n    var process = $data.getDensoProcess[0];\r\n    return process.id;\r\n}",
      "exposed" : false
    },
